doctype html
head
  meta(charset='utf-8')
  meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
  link(href='https://fonts.googleapis.com/css?family=Muli:300,400,600,700,800,900', rel='stylesheet')
  link(rel='stylesheet' href='css/main.css')
  title GiveawayApp


nav.navbar.navbar-expand-lg.navbar-dark.navbar-transparent
  a.navbar-brand(href='index.html')
    <img src="img/logo.png" width="125" class="d-inline-block align-top" alt="">
  button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarSupportedContent' aria-controls='navbarSupportedContent' aria-expanded='false' aria-label='Toggle navigation')
    span.navbar-toggler-icon
  #navbarSupportedContent.collapse.navbar-collapse
    ul.navbar-nav.ml-auto
      li.nav-item.active
        a.nav-link(href='#')
          | Giveaways 
          span.sr-only (current)
      li.nav-item
        a.nav-link(href='#') Winners
      li.nav-item
        a.nav-link(href='#') Pricing
      li.nav-item
        button.btn.btn-success(type='button') Join Now


section.jumbotron


section.giveaway-prize-wrapper
  .container
    .row
      .col-xl
      .col-xl-11
        .giveaway-prize
          .row
            .giveaway-info-primary.col-lg-8
              .giveaway-save-button.d-lg-none
                button.btn.btn-save(data-toggle='tooltip', data-html='true', title='<div class="display-4">Save Giveaway</div>')
              h2.h2 Amazon Echo
              p.subheading Smart Bluetooth Speaker & Assistant
              .giveaway-image-wrap
                img.img-scale(src="https://webobjects2.cdw.com/is/image/CDW/4583400?$product-main$")
            .giveaway-info-secondary.col-lg-4
              .giveaway-save-button.d-none.d-lg-block
                button.btn.btn-save(data-toggle='tooltip', data-html='true', title='<div class="display-4">Save Giveaway</div>')
              .giveaway-metadata-wrap
                .metadata-item
                  .display-4 Giveaway Ends In
                  span.giveaway-time-remaining.d-flex
                    img.mr-2(src="svg/clock.svg")
                    span.display-3 3 Days
                .metadata-item
                  .row
                    div.prize-value-wrapper.col-6
                      .display-4 Prize Value
                      .prize-value.display-3 $80
                    div.col-6
                      .display-4 You Pay
                      .prize-value.display-3.text-success
                        b $0
                .metadata-item
                  .display-4 Entry Method
                  span.giveaway-time-remaining.d-flex
                    img.mr-3(src="svg/gleam.svg")
                    span.display-3 Gleam
              .giveaway-cta-wrap
                a.btn.btn-success.btn-icon.btn-block.btn-lg.mb-4(href="#") 
                  span.icon.icon-ticket
                  | Enter Giveaway
                a.btn.btn-outline-secondary-alt.btn-icon.btn-block.btn-lg(href="#")
                  | Get it on
                  span.icon.icon-amazon
      .col-xl


section.giveaway-results-main.container
  .row.justify-content-between.mb-3
    .col-7
      h4.h4 Giveaways for you
    .col-5.text-right
      button.btn.btn-secondary.btn-sm(type='button') See all giveaways
  .row
    .col-md-4
      .giveaway-tn-item
        .giveaway-tn-header
          span.giveaway-tn-time-remaining(data-toggle='tooltip', data-html='true', title='<b>Giveaway-tn ends in:</b>')
