nav.navbar.navbar-expand-lg.navbar-dark.navbar-transparent
  a.navbar-brand(href='index.html')
    <img src="img/logo.png" width="125" class="d-inline-block align-top" alt="">
  button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarSupportedContent' aria-controls='navbarSupportedContent' aria-expanded='false' aria-label='Toggle navigation')
    span.navbar-toggler-icon
  #navbarSupportedContent.collapse.navbar-collapse
    ul.navbar-nav.ml-auto
      li.nav-item.active
        a.nav-link(href='#')
          | Giveaways 
          span.sr-only (current)
      li.nav-item
        a.nav-link(href='#') Winners
      li.nav-item
        a.nav-link(href='#') Pricing
      li.nav-item
        button.btn.btn-success(type='button')
          i.fa.fa-pencil.mr-2
          | Join Now


section.jumbotron
  .container
    .text-center.text-white
      p.lead The best way to find giveaways
      h1.h1 Enter Giveaways, Win Prizes
      button.btn.btn-icon-play.btn-sm
        img.mr-2(src="svg/play.svg")
        | How it Works
    .giveaway-search
      form.form-inline
        .form-item.col-md-12.col-lg-6
          label.display-4 Search
          .icon-form
            i.fa.fa-search.text-graylight
            input#inlineFormInputName.form-control(type='text' placeholder='Search for prizes')
        .form-item.col-6.col-sm-4.col-md-4.col-lg-2
          label.display-4 Category
          select#inlineFormCustomSelectPref.custom-select.my-1.mr-md-2
            option(selected='') All
            option(value='1') One
            option(value='2') Two
            option(value='3') Three
        .form-item.col-6.col-sm-4.col-md-4.col-lg-2
          label.display-4 Entry Method
          select#inlineFormCustomSelectPref.custom-select.my-1.mr-md-2
            option(selected='') All
            option(value='1') One
            option(value='2') Two
            option(value='3') Three
        .form-item.custom-control.custom-checkbox.col-sm-4.col-md-4.col-lg-2
          input#customControlInline.custom-control-input(type='checkbox')
          label.display-4.custom-control-label(for='customControlInline') Hide Entered


