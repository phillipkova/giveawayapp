include includes/_head.pug
  
body
  include includes/_header.pug

  section.jumbotron
    .container
      .text-center.text-white
        p.lead The best way to find giveaways
        h1.h1 Enter Giveaways, Win Prizes
        button.btn.btn-icon-play.btn-sm
          img.mr-2(src="svg/play.svg")
          | How it Works
      .giveaway-search
        form.form-inline
          .form-item.search-form.col-md-12.col-lg-6
            label.display-4 Search
            .icon-form
              i.fa.fa-search.text-graylight
              input#inlineFormInputName.form-control(type='text' placeholder='Search for prizes')
          .form-item.form-selector.col-6.col-lg-3
            label.display-4 Category
            select#inlineFormCustomSelectPref.custom-select.my-1.mr-md-2
              option(selected='') Category
              option(value='1') One
              option(value='2') Two
              option(value='3') Three
          .form-item.form-selector.col-6.col-lg-3
            label.display-4 Entry Method
            select#inlineFormCustomSelectPref.custom-select.my-1.mr-md-2
              option(selected='') Entry Method
              option(value='1') One
              option(value='2') Two
              option(value='3') Three
          .form-item.custom-control.custom-checkbox.col-12
            input#customControlInline.custom-control-input(type='checkbox')
            label.custom-control-label.text-muted(for='customControlInline') Hide Entered

  section.giveaway-results-main.container
    .row.justify-content-between.mb-3
      .col
        h4.h4 Hottest Giveaways
    .row
      .col-md-4
        .giveaway-tn-item.tn-lg
          .giveaway-tn-header
            span.giveaway-tn-time-remaining(data-toggle='tooltip', data-html='true', title='<div class="display-4">Time Remaining</div>')
              i.fa.fa-clock-o
              span 3 Days
            span.giveaway-tn-entry-method(data-toggle='tooltip', data-html='true', title='<div class="display-4">Entry Method</div><div class="text-left text-white">Gleam</div>')
              img(src="http://appcenter.bronto.com/wp-content/uploads/2017/03/9.1_CORP_appCenter_icon_gleam@2x.png")
          .giveaway-tn-product-image
            img(src="https://webobjects2.cdw.com/is/image/CDW/4583400?$product-main$")
          .giveaway-tn-titles
            .giveaway-tn-prize-name Amazon Echo
            .giveaway-tn-value $200 Value
          .giveaway-tn-cta
            a.btn.btn-primary.btn-block(href="prize.html") View Details
